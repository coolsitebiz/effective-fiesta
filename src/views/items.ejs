<html>

<head>
  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>

<body>
  <div class="container">
    <div class="jumbotron"><%for (let i=0; i<users.length; i++) {%>
      <%
        let certArray = users[i].certificates;
        let outputArray = [];
        certArray.forEach((item) => {
          outputArray.push(item.certificate);
        })
      %>
      <li><%=users[i].name%><br />
        <% for(let cert of users[i].certificates) { %>
        <li style="margin-left: 25px;"><%= cert.certificate %></li>
        <% } %>
      </li>
      <%}%>
    </div>
  </div>


  <script src="/js/jquery.min.js"></script>
  <script src="/js/bootstrap.min.js"></script>

</body>

</html>